{
    "customizeUI.activityBar": "wide",
    "customizeUI.titleBar": "inline",
    "customizeUI.stylesheet": {
        ".monaco-workbench .part.editor>.content .editor-group-container>.title .tabs-container>.tab.sizing-shrink>.monaco-icon-label>.monaco-icon-label-container": "line-height: 34px !important",
        ".monaco-workbench .part.editor>.content .editor-group-container>.title": "height: 34px !important"
    },
    "workbench.tree.indent": 16,
    "java.semanticHighlighting.enabled": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.requirements.JDK11Warning": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/Users/pedrogutierrez/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "breadcrumbs.enabled": true,
    "editor.minimap.enabled": false,
    "window.zoomLevel": 0,
    "elixirLS.fetchDeps": false,
    "terminal.integrated.fontWeightBold": "normal",
    "java.imports.gradle.wrapper.checksums": [
        {
            "sha256": "e24551a3b3f98866d26439148c81613d7677ea28bb8e135d27a13dfcbe31f0b0",
            "allowed": true
        }
    ],
    "sqltools.connections": [],
    "terminal.integrated.scrollback": 2000,
    "explorer.openEditors.visible": 1,
    "workbench.colorCustomizations": {
        "activityBar.activeBorder": "#161821",
        "editorGutter.background": "#161821",
        "editor.findMatchBackground": "#2D323F",
        "editor.findMatchHighlightBackground": "#2D323F",
        "sideBarSectionHeader.background": "#161821",
        "terminal.background": "#11121a",
    },
    //"workbench.iconTheme": "Monokai Pro Icons",
    "go.formatTool": "goimports",
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "scm.diffDecorations": "none",
    "python.languageServer": "Pylance",
    "java.project.importHint": false,
    "scm.defaultViewMode": "tree",
    "editor.renderWhitespace": "none",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "Monaco, JetBrains Mono NL, Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.lineHeight": 1.2,
    "workbench.startupEditor": "newUntitledFile",
    "editor.formatOnSave": true,
    "[java]": {
        "editor.formatOnSave": false
    },
    "[erlang]": {
        "editor.formatOnSave": false
    },
    "python.showStartPage": false,
    "hediet.vscode-drawio.local-storage": "",
    "elixirLS.suggestSpecs": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
                "settings": {
                    "foreground": "#84c689"
                }
            }
            // {
            //     "scope": "variable.other.constant.elixir",
            //     "settings": {
            //         "foreground": "#84a0c6"
            //     }
            // }
        ]
    },
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/Library/Java/JavaVirtualMachines/adoptopenjdk-8.jdk/Contents/Home",
        },
        {
            "name": "JavaSE-11",
            "path": "/Library/Java/JavaVirtualMachines/adoptopenjdk-11.jdk/Contents/Home",
        },
    ],
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.refactor.renameFromFileExplorer": "autoApply",
    "files.associations": {
        "*.hcl": "hcl"
    },
    "window.closeWhenEmpty": true,
    "window.titleBarStyle": "native",
    "workbench.statusBar.visible": true,
    "workbench.colorTheme": "Iceberg"
